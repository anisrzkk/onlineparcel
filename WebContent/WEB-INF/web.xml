<servlet-name>R5</servlet-name>
<url-pattern>/UpdateStaff</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>R6</servlet-name>
<url-pattern>/displayimage</url-pattern>
</servlet-mapping>


