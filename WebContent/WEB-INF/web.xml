</servlet-mapping>

<servlet-mapping>
<servlet-name>R3</servlet-name>
<url-pattern>/ParcelDisplay</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>R4</servlet-name>
<url-pattern>/PickUp</url-pattern>
</servlet-mapping>

