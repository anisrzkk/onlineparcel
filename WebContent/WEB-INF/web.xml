<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet>
<servlet-name>R1</servlet-name>
<servlet-class>OPF.RegisterStaff</servlet-class>
</servlet>

<servlet>
<servlet-name>R2</servlet-name>
<servlet-class>OPF.KeyIn</servlet-class>
</servlet>

<servlet>
<servlet-name>R3</servlet-name>
<servlet-class>OPF.ParcelDisplay</servlet-class>
</servlet>

<servlet>
<servlet-name>R4</servlet-name>
<servlet-class>OPF.PickUp</servlet-class>
</servlet>

